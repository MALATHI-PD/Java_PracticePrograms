package practice_javaa;
class Counter{
	static int count=0;
	int instancenumber=0;
	Counter(){
	count+=1;
	instancenumber+=1;
	}
	void display() {
		System.out.println("Static Count:"+count);
		System.out.println("Inum:"+instancenumber);	
	}
}

public class Stat_tic {
	public static void main(String args[]) {
	Counter obj=new Counter();
	Counter obj1=new Counter();
	Counter obj2=new Counter();
	obj.display();
	obj1.display();
	obj2.display();
	}
}





output:


Static Count:3
Inum:1
Static Count:3
Inum:1
Static Count:3
Inum:1

